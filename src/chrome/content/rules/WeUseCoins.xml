<ruleset name="WeUseCoins">
    <target host="weusecoins.com" />
    <target host="*.weusecoins.com" />

    <rule from="^https?://weusecoins\.com/"
        to="https://www.weusecoins.com/" />
    <rule from="^http://([^/:@]+)?\.weusecoins\.com/"
        to="https://$1.weusecoins.com/" />
</ruleset>
