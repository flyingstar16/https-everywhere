<!--
	Fully covered subdomains:

		- (www.)
		- cloud

-->
<ruleset name="Digital Ocean.com">

	<target host="digitalocean.com" />
	<target host="*.digitalocean.com" />


	<securecookie host="^(?:cloud|www)?\.digitalocean\.com$" name=".+" />


	<rule from="^http://(cloud\.|www\.)?digitalocean\.com/"
		to="https://$1digitalocean.com/" />

</ruleset>
