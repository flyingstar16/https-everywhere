<ruleset name="Zuger Kantonalbank">
    <target host="zugerkb.ch" />
    <target host="*.zugerkb.ch" />

    <securecookie host="^(.*\.)?zugerkb\.ch$" name=".+" />

    <rule from="^https?://zugerkb\.ch/"
        to="https://www.zugerkb.ch/"/>
    <rule from="^http://([^/:@]+)?\.zugerkb\.ch/"
        to="https://$1.zugerkb.ch/" />
</ruleset>
