<!--
	CDN buckets:

		- loomio-uploads.s3.amazonaws.com


	Problematic subdomains:

		- blog	(wordpress)

-->
<ruleset name="Loomio.org (partial)">

	<target host="loomio.org" />
	<target host="*.loomio.org" />


	<securecookie host="^(?:www)?\.loomio\.org$" name=".+" />


	<rule from="^http://(www\.)?loomio\.org/"
		to="https://$1loomio.org/" />

</ruleset>
