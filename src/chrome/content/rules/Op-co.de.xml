<!--
	Problematic subdomains:

		- www	(cert only matches ^op-co.de)


	Fully covered subdomains:

		- (www.)	(www â†’ ^)
		- gallery

-->
<ruleset name="op-co.de" platform="cacert">

	<target host="op-co.de" />
	<target host="*.op-co.de" />


	<rule from="^http://(?:(gallery\.)|www\.)?op-co\.de/"
		to="https://$1op-co.de/" />

</ruleset>
