<!--
	Problematic subdomains:

		- ^ *

	* Refused

-->
<ruleset name="Kudelski Security">

	<target host="kudelskisecurity.com" />
	<target host="*.kudelskisecurity.com" />


	<securecookie host="^\.kudelskisecurity\.com$" name=".+" />


	<rule from="^http://(?:www\.)?kudelskisecurity\.com/"
		to="https://www.kudelskisecurity.com/" />

</ruleset>
