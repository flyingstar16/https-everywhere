<ruleset name="Yourmoney.ch">
    <target host="yourmoney.ch" />
    <target host="*.yourmoney.ch" />

    <securecookie host="^(.*\.)?yourmoney\.ch$" name=".+" />

    <rule from="^https?://yourmoney\.ch/"
        to="https://www.yourmoney.ch/"/>
    <rule from="^http://([^/:@]+)?\.yourmoney\.ch/"
        to="https://$1.yourmoney.ch/" />
</ruleset>
