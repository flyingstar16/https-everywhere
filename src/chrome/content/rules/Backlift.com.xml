<!--
	Fully covered subdomains:

		- (www.)
		- blog
		- docs

-->
<ruleset name="Backlift.com">

	<target host="backlift.com" />
	<target host="*.backlift.com" />


	<securecookie host="^(?:\w+\.)?backlift\.com$" name=".+" />


	<rule from="^http://((?:blog|docs|www)\.)?backlift\.com/"
		to="https://$1backlift.com/" />

</ruleset>
