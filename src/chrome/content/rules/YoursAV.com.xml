<ruleset name="YoursAV.com" default_off="self-signed">

	<target host="yoursav.com" />
	<target host="www.yoursav.com" />
	<target host="yoursav.net" />
	<target host="www.yoursav.net" />


	<rule from="^http://(?:www\.)?yoursav\.(com|net)/"
		to="https://yoursav.$1/" />

</ruleset>
