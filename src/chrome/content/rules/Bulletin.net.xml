<!--
	Other Bulletin.net rulesets:

		- Bulletin_Messenger.net.xml

-->
<ruleset name="Bulletin.net">

	<target host="bulletin.net" />
	<target host="*.bulletin.net" />


	<securecookie host="^(?:www)?\.bulletin\.net$" name=".+" />


	<rule from="^http://(www\.)?bulletin\.net/"
		to="https://$1bulletin.net/" />

</ruleset>
