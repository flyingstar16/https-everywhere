<ruleset name="WeGotWax.com">

	<target host="wegotwax.com" />
	<target host="*.wegotwax.com" />


	<securecookie host="^(?:www)?\.wegotwax.com$" name=".+" />


	<!--	^ redirects to www over http,
		so copy that behavior:
					-->
	<rule from="^http://(?:www\.)?wegotwax\.com/"
		to="https://www.wegotwax.com/" />

</ruleset>
