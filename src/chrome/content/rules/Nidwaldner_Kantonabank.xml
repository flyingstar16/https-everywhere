<ruleset name="Nidwaldner Kantonabank">
    <target host="nkb.ch" />
    <target host="*.nkb.ch" />

    <securecookie host="^(.*\.)?nkb\.ch$" name=".+" />

    <rule from="^https?://nkb\.ch/"
        to="https://www.nkb.ch/"/>
    <rule from="^http://([^/:@]+)?\.nkb\.ch/"
        to="https://$1.nkb.ch/" />
</ruleset>
