<!--
	Nonfunctional subdomains:

		- www	(redirects to http)

-->
<ruleset name="Cinfu.com (partial)">

	<target host="cinfu.com" />
	<target host="*.cinfu.com" />


	<securecookie host="^\.?panel\.cinfu\.com$" name=".+" />


	<rule from="^http://(panel\.)?cinfu\.com/"
		to="https://$1cinfu.com/" />

</ruleset>
