<!--
	Problematic subdomains:

		- ^	(http reply)


	Mixed content:

		- css on www from www *

		- Images, on www from:

			- www *
			- \d.gravatar.com *

		- Ads, on www from:

			- api.lanistaads.com *

	* Secured by us

-->
<ruleset name="Stunnish.com (false MCB)" platform="mixedcontent">

	<target host="stunnish.com" />
	<target host="*.stunnish.com" />


	<securecookie host="^\.stunnish\.com$" name=".+" />


	<rule from="^http://(?:www\.)?stunnish\.com/"
		to="https://www.stunnish.com/" />

</ruleset>
