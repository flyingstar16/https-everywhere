<ruleset name="Appenzeller Kantonalbank">
    <target host="appkb.ch" />
    <target host="*.appkb.ch" />

    <securecookie host="^(.*\.)?appkb\.ch$" name=".+" />

    <rule from="^https?://appkb\.ch/"
        to="https://www.appkb.ch/"/>
    <rule from="^http://([^/:@]+)?\.appkb\.ch/"
        to="https://$1.appkb.ch/" />
</ruleset>
