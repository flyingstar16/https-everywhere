<!--
	For rules causing false/broken MCB, see GetDigital.de-falsemixed.

-->
<ruleset name="getDigital.de (false MCB)" platform="mixedcontent">

	<target host="getdigital.de"/>
	<target host="www.getdigital.de"/>
	<target host="getdigital-blog.de" />
	<target host="www.getdigital-blog.de" />


	<rule from="^http://(?:www\.)?getdigital\.de/"
		to="https://www.getdigital.de/" />

	<rule from="^http://(?:www\.)?getdigital-blog\.de/"
		to="https://www.getdigital.de/blog/" />

</ruleset>
