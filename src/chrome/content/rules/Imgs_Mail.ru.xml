<ruleset name="imgs Mail.ru">

	<target host="*.imgsmail.ru" />


	<rule from="^http://(auto|cars|img|limg)\.imgsmail\.ru/"
		to="https://$1.imgsmail.ru/" />

	<rule from="^http://go\.imgsmail\.ru/"
		to="https://go.imgsmail.ru/" />

</ruleset>
